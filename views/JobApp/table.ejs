<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <style>
        a{
            text-align: center;
            text-decoration: none;
            color: black;
            font-family: Arial, Helvetica, sans-serif;
            padding: 5px;
        }
        a:hover{
            color: white;
            background-color: black;
        }
    </style>
</head>
<body>
    <table border="1">
        <tr>
            <th>CandId</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone No</th>
            <th>City</th>
            <th>State</th>
            <th>Gender</th>
            <th>VIEW</th>
        </tr>


       <% data.forEach((dt) => { %>
        <tr>
            <td><%= dt.candId %></td>
            <td><%= dt.fname %></td>
            <td><%= dt.lname %></td>
            <td><%= dt.email %></td>
            <td><%= dt.phoneNo %></td>
            <td><%= dt.city %></td>
            <td><%= dt.state %></td>
            <td><%= dt.gender %></td>
            <td><a href="user/<%= dt.candId %>">View</a></td>
       
        </tr>
       <% }) %>
    </table>

    
</body>
</html>